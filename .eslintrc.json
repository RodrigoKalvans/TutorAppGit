{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google", "next"],
  "root": true,
  "rules": {
    "quotes": ["error", "double"],
    "require-jsdoc": "warn",
    "new-cap": "off",
    "linebreak-style": "off",
    "max-len": "off"
  }
}
