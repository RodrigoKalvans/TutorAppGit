{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "quotes": ["error", "double"],
    "require-jsdoc": "warn",
    "new-cap": "off",
    "linebreak-style": "off"
  }
}
